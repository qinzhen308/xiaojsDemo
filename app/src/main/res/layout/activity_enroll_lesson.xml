<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <include layout="@layout/layout_my_course_search"/>

    <View
            android:id="@+id/filter_line"
            style="@style/DividerHorizontal"/>

    <cn.xiaojs.xma.common.pulltorefresh.core.PullToRefreshSwipeListView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/enroll_lesson_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none"
            android:divider="@android:color/transparent"
            android:listSelector="@android:color/transparent"
            android:dividerHeight="@dimen/px20"
            android:orientation="vertical"/>
</LinearLayout>